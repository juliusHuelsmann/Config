
xterm*fastScroll: true

! cursor
Xcursor.theme: smalldot

! Colorsheme
*color0: #3f3f3f
*color1: #705050
*color2: #60b48a
*color3: #dfaf8f
*color4: #506070
*color5: #dc8cc3
*color6: #8cd0d3
*color7: #dcdccc
*color8: #709080
*color9: #dca3a3
*color10: #c3bf9f
! *color11: #f0dfaf
*color11: #021021
*color12: #94bff3
!*color13: #011009
*color13: #120203
!*color13: #ec93d3
*color14: #93e0e3
*color15: #ffffff

! general st settings (which can be overwritten in custom xresources
st.font: hack:pixelsize=19:antialias=true:autohint=true
st.alpha: 0.85
st.alphaUnfocussed: 0.55

! custom xresources for each machine (due to different resolutions etc).
#include "custom.xresources"
